{
    "manifest_version": 3,
    "name": "Seopyo",
    "description": "A chrome extension to keep track of webtoon chapters",
    "version": "1.0",
    "icons": {
      "128": "seopyo.png"
    },
    "background": {
      "service_worker": "background.js"
    },

    "action": {
      "default_popup": "popup.html",
      "default_popup_css": "popup.css"
    },
    "permissions": [
      "tabs",
      "webNavigation",
      "storage"
    ]
}